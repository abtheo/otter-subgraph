{
  "name": "otterclam-subgraph",
  "version": "0.1.0",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "auth": "graph auth --product hosted-service",
    "deploy": "yarn build && graph deploy --product hosted-service abtheo/otter-development",
